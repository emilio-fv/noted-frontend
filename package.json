{
  "name": "noted-frontend",
  "version": "1.0.0",
  "description": "The frontend client for the application Noted",
  "private": true,
  "scripts": {
    "start": "npm run docker:build && npm run docker:start",
    "stop": "npm run docker:stop && npm run docker:remove",
    "dev": "webpack serve --config ./webpack.dev.js",
    "test": "jest",
    "test-v": "jest --verbose",
    "build": "webpack --config ./webpack.prod.js",
    "docker:build": "docker build -t noted-client .",
    "docker:start": "docker run -p 8000:8000 -v ${PWD}:/app -v /app/node_modules -d --name noted-client-dev noted-client",
    "docker:stop": "docker stop noted-client-dev",
    "docker:remove": "docker rm noted-client-dev && docker image rm noted-client"
  },
  "author": "Emilio Vazquez - emiliofv.sd@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.20",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint-plugin-jest": "^27.4.3",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.7.6",
    "msw": "^2.0.8",
    "react-test-renderer": "^18.2.0",
    "redux-mock-store": "^1.5.4",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.9",
    "@mui/material": "^5.14.10",
    "@mui/x-date-pickers": "^6.16.0",
    "@reduxjs/toolkit": "^1.9.7",
    "dayjs": "^1.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.16.0",
    "redux-persist": "^6.0.0",
    "undici": "^5.28.1"
  }
}
